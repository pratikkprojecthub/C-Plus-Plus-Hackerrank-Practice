#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

// start of the code
int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
 
 int b;
 cin >> b;
 int arr[b];
 for (int i=0;i<b;i++)
 cin >> arr[i];
 for (int i=b-1;i>=0;i--)
 cout << arr[i] << " ";
    return 0;
}
